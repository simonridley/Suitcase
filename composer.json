{
    "name": "juststeveking/suitcase",
    "type": "library",
    "description": "A simple PHP SDK that takes the hassle out of building your own SDK",
    "keywords": [
        "JustSteveKing",
        "Suitcase"
    ],
    "homepage": "https://github.com/JustSteveKing/Suitcase",
    "license": "MIT",
    "authors": [
        {
            "name": "Steve McDougall",
            "email": "juststevemcd@gmail.com",
            "homepage": "https://juststeveking.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.4",
        "guzzlehttp/guzzle": "^6.5",
        "league/container": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "JustSteveKing\\Suitcase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustSteveKing\\Suitcase\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test:unit": "phpdbg -qrr ./vendor/bin/phpunit --coverage-html build/coverage-report -d memory_limit=512M --testdox",
        "test": [
            "@test:unit"
        ],
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
